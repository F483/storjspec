            "type": "string",
            "pattern": "^[13][a-km-zA-HJ-NP-Z0-9]{26,33}$"
        },
        "farmer_address": {
            "type": "string",
            "format": ["ipv4", "ipv6", "hostname"]
        },
        "farmer_port": {
            "type": "integer",
            "minimum": 1,
            "maximum": 65535
        },
        "farmer_signature": {
            "type": "string"
        },
